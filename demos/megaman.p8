pico-8 cartridge // http://www.pico-8.com
version 22
__lua__
function _init()
	idx = 0
	mag = 1
end

function _update()
	idx += 3*mag
	if idx > 6 then
		idx = 3
		mag *= -1
	end
	if idx < 0 then
		idx = 3
		mag *= -1
	end
end

function _draw()
	cls(5)
	spr(idx, 52, 52, 3, 3)
end

